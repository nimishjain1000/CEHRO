<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arora.cehroindia">
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



        <activity android:name=".EducatingIndiaProg"
            android:label="Educating India"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".PadhoPadhaoProgramActivity"
            android:label="Padho-Padhao Program"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".LivelihoodProgActivity"
            android:label="Livelihood Program"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".TargetTwentyProgActivity"
            android:label="Target Twenty 20"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".HealthProgActivity"
            android:label="Health And Hygeine Practices"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ArtisticIndiaActivity"
            android:label="Artistic India"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>